{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "标签一键保存",
  "version": "2.0.2",
  "manifest_version": 2,
  "description": "一键保存所有标签，一键打开所有标签",
  "icons": {
    "16": "icon_16.png",
    "48": "icon.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "permissions": [
    "tabs",
    "storage",
	"syncFileSystem"
  ]
}